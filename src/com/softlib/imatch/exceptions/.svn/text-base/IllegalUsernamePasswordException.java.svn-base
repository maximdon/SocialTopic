package com.softlib.imatch.exceptions;

public class IllegalUsernamePasswordException extends RuntimeException {
	private static final long serialVersionUID = -6777995363986994138L;
	
	private String msg;

	public void setMsg(String msg) {
		this.msg = msg;
	}

	public String getMsg() {
		return msg;
	}
	
	public IllegalUsernamePasswordException(String string) {
		this.msg = string;
	}

	@Override
	public String getMessage() {
		// TODO Auto-generated method stub
		return getMsg();
	}
}
